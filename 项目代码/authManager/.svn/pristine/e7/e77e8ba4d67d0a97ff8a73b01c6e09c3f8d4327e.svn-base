body {
    font-family:verdana,helvetica,arial,sans-serif;
    font-size:12px;
    margin:0;
}
h2 {
    font-size:18px;
    font-weight:bold;
    margin:0;
    margin-bottom:15px;
}
.myTable{
	width:100%;
	border-collapse: collapse;
}
.myTable td {
	padding: 2px;
	border-color: #ccc;
	border-width: 1px;
	border-style: dotted;
}